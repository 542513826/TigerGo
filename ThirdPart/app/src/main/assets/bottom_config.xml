<?xml version="1.0" encoding="utf-8"?>
<bottom>
    <!--index    底部导航按钮坐标,通过数字来代表当前位于第X个位置,从1开始,各个导航之间的数字必须连续,不能出现1,2,4等情况-->
    <!--label    底部导航按钮名称,字数限制为5个字-->
    <!--picName  底部导航按钮图片名称-->
    <!--fragment 底部导航按钮对应的fragment全包名-->
    <tab>
        <index>1</index>
        <label>应用</label>
        <picName>tab_appshop</picName>
        <fragment-class>com.smartdot.mobile.portal.fragment.appFragment</fragment-class>
    </tab>
    <tab>
        <index>2</index>
        <label>消息</label>
        <picName>tab_message</picName>
        <fragment-class>com.smartdot.mobile.portal.fragment.ConversionListFragment</fragment-class>
    </tab>
    <tab>
        <index>3</index>
        <label>组织</label>
        <picName>tab_address</picName>
        <fragment-class>com.smartdot.mobile.portal.fragment.OrganizationFragment</fragment-class>
    </tab>
    <tab>
        <index>4</index>
        <label>我的</label>
        <picName>tab_my</picName>
        <fragment-class>com.smartdot.mobile.portal.fragment.myFragment</fragment-class>
    </tab>
</bottom>